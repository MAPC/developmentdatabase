<div class="ui grid container">
  <div class="row">
    <div class="ten wide column">
      {{partial "developments/forms/general"}}
    </div>
    <div class="six wide column">
      {{#if model.id}}
        {{partial "developments/forms/development_team"}}
      {{/if}}
    </div>
  </div>
  <div class="row">
    <div class="sixteen wide column">
      {{partial "developments/forms/spatial"}}
    </div>
  </div>
  <div class="row">
    <div class="ten wide centered column">

      {{partial "developments/forms/physical"}}

      {{partial "developments/forms/residential"}}

      {{partial "developments/forms/commercial"}}

      {{partial "developments/forms/employment"}}

      <button class="ui primary button" {{action "save" model}}>
        Propose Changes
      </button>
    </div>
  </div>
</div>
